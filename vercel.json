{
  "version": 2,
  "builds": [
    {
      "src": "api/json-server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/json-server.mjs"
    }
  ],
  "env": {
    "NODE_VERSION": "14.x" 
  }
}
